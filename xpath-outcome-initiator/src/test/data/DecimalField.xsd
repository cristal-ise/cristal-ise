    <xs:element name="DecimalField">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="1" maxOccurs="1" name="value" type='xs:decimal' />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>